{
    "name": "essex-interactive/nova-styling",
    "version": "v2.3.7",
    "description": "A Responsive Laravel Nova essex-interactive Theme based on 'A Responsive Laravel Nova theme'. With dark mode :)",
    "keywords": [
        "Laravel Nova Theme",
        "essex-interactive Theme",
        "Dark Mode",
        "Laravel Nova Dark Mode",
        "essex-interactive",
        "laravel",
        "nova",
        "Responsive Laravel Nova Theme"
    ],
    "homepage": "https://github.com/essex-interactive/nova-styling",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev/",
            "role": "Developer"
        },
        {
            "name": "LTKort",
            "email": "lars@kort.dev",
            "homepage": "https://kort.dev/",
            "role": "Developer"
        },
        {
            "name": "Essex Interactive",
            "homepage": "https://github.com/Essex-Interactive",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "marshmallow/categorise-resources": "^1.0.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "EssexInteractive\\NovaStyling\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EssexInteractive\\NovaStyling\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EssexInteractive\\NovaStyling\\ThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
