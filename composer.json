{
    "name": "marshmallow/nova-styling",
    "description": "A Response Laravel Nova Marshmallow Theme.",
    "keywords": [
        "Laravel Nova Theme",
        "Marshmallow Theme",
        "Marshmallow"
    ],
    "homepage": "https://gitlab.com/marshmallow-packages/",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev"
        }
    ],
    "require": {
        "php": "^7.1",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\NovaStyling\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marshmallow\\NovaStyling\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\NovaStyling\\ThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
